{
  "name": "tsdx-lib",
  "author": "Olli Saarinen",
  "version": "1.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint src",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "dependencies": {
    "jwt-decode": "^3.1.2",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.21.1",
    "react": ">=16",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^3.0.1",
    "@rollup/plugin-image": "^2.0.6",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@storybook/addon-docs": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/addons": "^6.1.20",
    "@storybook/react": "^6.1.20",
    "@storybook/theming": "^6.1.21",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jwt-decode": "^3.1.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/storybook-react-router": "^1.0.1",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "gh-pages": "^3.1.0",
    "husky": "^5.1.2",
    "postcss": "^8.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "size-limit": "^4.9.2",
    "storybook-react-router": "^1.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "jest": {
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "module": "dist/tsdx-lib.esm.js",
  "size-limit": [
    {
      "path": "dist/tsdx-lib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tsdx-lib.esm.js",
      "limit": "10 KB"
    }
  ]
}
